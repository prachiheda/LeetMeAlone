{
    "manifest_version": 3, 
    "name": "LeetMeAlone", 
    "description": "Makes you do a LeetCode problem.", 
    "version": "1.0", 
    "action": {
        "default_popup": "popup/popup.html"
    }, 
    "icons":{
        "16": "images/icon-16.png", 
        "32": "images/icon-32.png",
        "128": "images/icon-128.png"
    }, 
    "background": {
        "service_worker": "background.js", 
        "type": "module"
    }, 
    "permissions":[
        "storage", 
        "alarms", 
        "notifications", 
        "tabs"
    ],
    "host_permissions": [
        "https://leetcode.com/graphql/"
    ]
}